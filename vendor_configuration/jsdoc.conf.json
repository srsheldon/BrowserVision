{
    "opts": {
        "template": "templates/default",
        "encoding": "utf8",
        "destination": "./doc/jsdoc",
        "package": "package.json",
        "recurse": true,
        "tutorials": "example"
    },
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "source": {
        "include": ["./"],
        "exclude": ["node_modules"],
        "includePattern": "\\.(js|mjs)$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "sourceType": "module",
    "tags": {
        "allowUnknownTags": true,
        "dictionaries": ["jsdoc","closure"]
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}